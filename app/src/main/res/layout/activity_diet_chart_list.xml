<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundhome"
    android:orientation="vertical"
    tools:context="com.example.asd.com.healthcare.DietChartList">
    <include
        layout="@layout/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <TableRow>
            <TextView
                android:gravity="center"
                android:text="Member Name"
                android:textColor="@color/white"
                android:paddingTop="16dip"
                android:paddingBottom="16dip"
                android:layout_weight=".7"
                android:background="#9b25af"
                />
            <TextView
                android:gravity="center"
                android:text="Diet Type"
                android:paddingTop="16dip"
                android:paddingBottom="16dip"
                android:textColor="@color/white"
                android:layout_weight="1"
                android:background="#ff760d"
                />
            />
            <TextView
                android:gravity="center"
                android:text="Diet Menu"
                android:paddingTop="16dip"
                android:paddingBottom="16dip"
                android:textColor="@color/white"
                android:layout_weight="1"
                android:background="#fec901"
                />
            />

        </TableRow>

    </TableLayout>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/DietListChart"
        >

    </ListView>

</LinearLayout>
